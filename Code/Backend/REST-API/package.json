{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "start": "npm run build && start node ./build/server.js",
    "dev": "npm run build:dev && start nodemon ./build/server.js",
    "build:dev": "start tsc --watch --project tsconfig.test.json",
    "build:dev:pipe": "tsc --project tsconfig.test.json",
    "build": "npx tsc --project tsconfig.prod.json",
    "test": "npm run build:dev:pipe && jest --runInBand",
    "test:dev": "npm run build:dev && start jest --watchAll --runInBand",
    "test:unit": "npm run build:dev:pipe && jest -c jest.config.unit.ts",
    "test:unit:dev": "start npm run test:unit -- --watchAll",
    "test:integration": "npm run build:dev:pipe && jest -c jest.config.integration.ts --runInBand",
    "test:integration:dev": "start npm run test:integration -- --watchAll",
    "lint": "eslint src server.ts --fix",
    "lint:pipe": "eslint src server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "typescript": "^5.3.3"
  }
}
